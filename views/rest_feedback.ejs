    <script>
        $(document).ready(function () {
            var socket = io();
            socket.on('add-order', (rid) => {
                if (rid == '<%= profile.rid %>') {
                    location.reload();
                }
            });
        });

    </script>
    <script>
        function hamburg() {
            $('.container').toggleClass('blur');
            $('.ham__items').toggleClass('ham__unhide');
            // $('.ham__items').toggleClass('.ham__items');

        };

        function xyz() {
            $('.profile__tab').toggleClass('hide');
            $('.container').toggleClass('blur');
        }
        function abc() {
            $('.profile__tab').toggleClass('hide');
            // $('.container').toggleClass('blur');
            $('.ham__items').toggleClass('ham__unhide');
        }


        $('#profile__tab__close').click(xyz);
        $('.profile ').click(xyz);

        $('.ham__profile ').click(abc);


        $('.ham').click(hamburg);
        $('#close').click(hamburg);

        $('.card__response__tab__accept').click(
            function (){
                $(this).css('display','none');
                
            }
        )
        
            $('.card__response__tab__delivered').click(
                function () {
                    console.log("eg");
                    $(this).text("✔ Delivered ");
                    $(this).attr("disabled","disabled");
                    

                }
            )

            $('#feedback__alert__close').click(
                function(){
                    $('.feedback__alert').addClass("hide");
                       $(".container").toggleClass("blur");
                }
            )
            $(".rest__feedback").click(
                function(){
                    $('.feedback__alert').removeClass("hide");
                    $(".container").toggleClass("blur");
                }
            )
      


    </script>
