    <script>
        function hamburg() {
            $('.container').toggleClass('blur');
            $('.ham__items').toggleClass('ham__unhide');
            // $('.ham__items').toggleClass('.ham__items');
 
        };
 
        function xyz() {
            $('.profile__tab').toggleClass('hide');
            $('.container').toggleClass('blur');
            $(".result").toggleClass("hide");
        }
        function abc() {
            $('.profile__tab').toggleClass('hide');
            // $('.container').toggleClass('blur');
            $('.ham__items').toggleClass('ham__unhide');
            $(".result").toggleClass("hide");
        }
 
 
        $('#profile__tab__close').click(xyz);
        $('.profile ').click(xyz);
 
        $('.ham__profile ').click(abc);
 
 
        $('.ham').click(hamburg);
        $('#close').click(hamburg);
 
        $('.menu__add__dishes button').click(
            function(){
                $(".add__dishes__popup").toggleClass("hide");
                 $('.container').toggleClass('blur');
 
            }
        )
 
              $('#add__dishes__popup__close').click(
                    function () {
                        $(".add__dishes__popup").toggleClass("hide");
                         $('.container').toggleClass('blur');
 
                    }
                )
 
 
    </script>
