       <div class="search__block">
            <div class="search__image">
 
            </div>
           
            <div class="search__tab">
                <p>Discover Best Restaurants in town</p>
                <form action="/search", method="GET">
                    <div class="search__tab__1">
                        <input type="text" placeholder="Search Restaurants/Dishes/Categories" name="q">
                        <select name="type" id="">
                            <option value="rest">Restaurants</option>
                            <option value="dish">Dishes</option>
                            <option value="cat">Category</option>
                       
                        </select>
                    </div>
                    <div class="search__tab__2">
                        <button type="submit">Search </button>
                    </div>
                   
                </form>
 
            </div>
       </div>
       
       <div class="result">
        <div class="user__feed">
       
       
            <% if(check == 'true') { %>
                <div class="rest__card">No Results</div>
            <% } else if(check == 'false') { %>
                <% searchresults.forEach(res => { %>
                    <a href="/browse/<%= res.rid %>">
                        <div class="rest__card">
                            <img width="229px" src="./frontend/images/rest_img_<%= res.rid %>.jpg" alt="">
                            <div class="rest__name"><%= res.rname %></div>
                            <div class="rest__rating">
                                <svg width="15px" fill="white" id="Capa_1" enable-background="new 0 0 448.941 448.941"
                                    viewBox="0 0 448.941 448.941" xmlns="http://www.w3.org/2000/svg">
                                    <path
                                        d="m448.941 168.353h-161.338l-63.132-168.353-63.132 168.353h-161.339l121.478 106.293-65.36 174.295 168.353-84.176 168.353 84.176-65.361-174.296z" />
                                </svg><%= res.rating %></div>
                            <div class="rest__category">
                                <p><%= res.category %></p>
                            </div>
                            <div class="rest__location"><%= res.address %></div>
                        </div>
                    </a>
                <% }); %>
            <% } %>
       
        </div>
       </div>
       
   
 
 
 
